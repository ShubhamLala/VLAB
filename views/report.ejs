          ['String', Number('<%= result.c_titles[0].string %>'), Number('<%= result.c_titles[0].corr_string %>'), Number('<%= result.c_titles[0].incorr_string %>')],
          ['Library Func', Number('<%= result.c_titles[0].libraryfunction %>'), Number('<%= result.c_titles[0].corr_libraryfunction %>'), Number('<%= result.c_titles[0].incorr_libraryfunction %>')],
