          ['Operator', Number('<%= result.c_titles[0].operator %>'), Number('<%= result.c_titles[0].corr_operator %>'), Number('<%= result.c_titles[0].incorr_operator %>')],
          ['Mem Alloc', Number('<%= result.c_titles[0].memoryallocation %>'), Number('<%= result.c_titles[0].corr_memoryallocation %>'), Number('<%= result.c_titles[0].incorr_memoryallocation %>')],
          ['Control Structure', Number('<%= result.c_titles[0].controlstructure %>'), Number('<%= result.c_titles[0].corr_controlstructure %>'), Number('<%= result.c_titles[0].incorr_controlstructure %>')],
          ['Typedef', Number('<%= result.c_titles[0].typedef %>'), Number('<%= result.c_titles[0].corr_typedef %>'), Number('<%= result.c_titles[0].incorr_typedef %>')],
          ['Expressions', Number('<%= result.c_titles[0].expressions %>'), Number('<%= result.c_titles[0].corr_expressions %>'), Number('<%= result.c_titles[0].incorr_expressions %>')],
          ['Preprocessor', Number('<%= result.c_titles[0].preprocessor %>'), Number('<%= result.c_titles[0].corr_preprocessor %>'), Number('<%= result.c_titles[0].incorr_preprocessor %>')],
