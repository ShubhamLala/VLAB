        ]);

        var options = {
          chart: {
            title: 'C',
            subtitle: 'Topic wise Correct and Incorrect answering',
          },
          colors: ['#FF9B1D','#7CFC00','#ff0000'],
          bars: 'vertical' // Required for Material Bar Charts.
        };

        var chart = new google.charts.Bar(document.getElementById('c_chart'));

        chart.draw(data, google.charts.Bar.convertOptions(options));
      }
    </script>

